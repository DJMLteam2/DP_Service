<!--여기 이부분에서 받습니다.-->
{{#locations}}
    <input type="hidden" name="question" id="MAP_LON" value="{{lon}}" />
    <input type="hidden" name="question" id="MAP_LAT" value="{{lat}}" />
{{/locations}}
<!--배열을 받았는지 테스트 용도-->
<div id="coordinates"></div>
<script>
    // 자바스크립트 코드 작성
    const lonInputs = document.querySelectorAll('#MAP_LON');
    const latInputs = document.querySelectorAll('#MAP_LAT');

    // 각 입력 요소의 값을 배열로 추출
    const lonArray = Array.from(lonInputs).map(input => input.value);
    const latArray = Array.from(latInputs).map(input => input.value);

    // 배열 값을 화면에 출력
    const coordinatesDiv = document.getElementById('coordinates');

    for (let i = 0; i < lonArray.length; i++) {
        const coordinateString = `Lon: ${lonArray[i]}, Lat: ${latArray[i]}`;
        coordinatesDiv.innerHTML += `<p>${coordinateString}</p>`;
    }
</script>
<!--여기 이부분에서 받습니다.-->

	<script>
    document.addEventListener('DOMContentLoaded', function () {
        var swiper = new Swiper('.swiper-container', {
            effect: 'coverflow',
            grabCursor: true,
            centeredSlides: true,
            slidesPerView: 'auto',
            autoplay: {
            // 자동재생 여부
                delay: 4000, // 시작시간 설정
            },
      
            coverflowEffect: {
                rotate: 35,
                stretch: 0,
                depth: 50,
                modifier: 1,
                slideShadows: true,
            },
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
        });
    });

    // Swiper 슬라이드 내의 링크 요소에 대한 클릭 이벤트 처리
    var slideLinks = document.querySelectorAll('.swiper-container .slide-link');
    slideLinks.forEach(function (link) {
        link.addEventListener('click', function (event) {
            // 클릭 시에 기본 동작(링크 이동) 방지
            event.preventDefault();
            // 링크의 href 속성 값으로 페이지 이동
            window.location.href = link.getAttribute('href');
        });
     });


    </script>
